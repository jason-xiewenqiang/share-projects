{
  "name": "@xbro/xbro-utils",
  "version": "1.0.1",
  "description": "Xbrother 前端工具函数库",
  "type": "module",
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./*": "./*"
  },
  "typings": "./lib/types/index.d.ts",
  "scripts": {
    "clean": "rimraf ./lib",
    "test": "vitest",
    "lint": "eslint src --fix --ext .ts,.js",
    "coverage": "vitest run --coverage",
    "docs": "rimraf ./docDist && typedoc && npm run vp:build",
    "vp:dev": "vitepress dev .",
    "vp:build": "vitepress build .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "build": "npm run clean && rollup --config rollup.config.js"
  },
  "keywords": [
    "xbro-utils",
    "rollup",
    "vitest",
    "vuepress",
    "typescript"
  ],
  "author": "xiewenqiang@xbrother.com",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitest/coverage-v8": "^1.2.1",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.7",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.3.3",
    "vitepress": "^1.0.0-alpha.75",
    "vitest": "^1.2.1",
    "vue": "^3.2.47"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix"
  }
}
